{
  "name": "parcel-react-server-starter",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "targets": {
    "server": {
      "source": "app/server.ts",
      "context": "react-server",
      "scopeHoist": false,
      "includeNodeModules": {
        "express": false,
        "better-sqlite3": false
      }
    }
  },
  "scripts": {
    "build": "parcel build",
    "clean": "rm -rf dist node_modules .parcel-cache .react-router .react-router-parcel",
    "dev": "parcel",
    "start": "node ./dist/server/server.js",
    "vc-build": "VERCEL=true vite build",
    "vc-release": "vercel deploy --prod --prebuilt"
  },
  "dependencies": {
    "@mjackson/node-fetch-server": "^0.6.1",
    "@parcel/runtime-rsc": "^2.15.0",
    "@ryanflorence/async-provider": "^0.0.1",
    "@ryanflorence/batch-loader": "^0.0.1",
    "better-sqlite3": "^11.9.1",
    "cookie": "^1.0.2",
    "express": "^4.21.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "0.0.0-experimental-3f68c7c20",
    "react-server-dom-parcel": "^19.1.0",
    "react-server-dom-webpack": "latest"
  },
  "devDependencies": {
    "@hiogawa/vite-rsc": "latest",
    "@react-router/dev": "0.0.0-experimental-3f68c7c20",
    "@tailwindcss/postcss": "^4.1.4",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.7",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^4",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vercel/nft": "^0.29.3",
    "@vitejs/plugin-react": "^4.4.1",
    "parcel": "^2.15.0",
    "parcel-config-react-router-experimental": "^1.0.13",
    "postcss": "^8.2.1",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.4",
    "vite": "^6.3.5",
    "vite-plugin-inspect": "^11.0.1"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
}
